{
  "name": "ddimitrov/pxb-zend-session",
  "description": "Zend Session module for PXB",
  "license": "MIT",
  "extra": {
    "pxb-config": "\\PXB\\Module\\Zend\\Session\\SessionModuleConfig"
  },
  "autoload": {
    "psr-4": {
      "PXB\\Module\\Zend\\Session\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PXB\\Module\\Test\\": "test/"
    }
  },
  "require": {
    "zendframework/zend-session": "2.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "phpspec/prophecy": "1.5.*",
    "zendframework/zend-expressive": "~1.0@rc || ^1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-text --coverage-html=build/"
  }
}
